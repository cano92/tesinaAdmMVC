{% extends 'base.html.twig' %}

{% block title %}Tesina en Curso {% endblock %}


{% block body %} Hola Vue!!!
<script src="https://unpkg.com/vue"></script>

    <h4> vue Js example</h4>

    <div id="app">
        <h2> <% message  %> </h2>
    </div>



  <script>
    var app = new Vue({
      el: '#app',
      data: {
        message: 'Hola Vue!!!'
        
      },
      delimiters: ["<%","%>"]
    })
  </script>



{% endblock %}